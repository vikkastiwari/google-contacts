{
    "version": 2,
    "builds": [
        {
            "src": "./index.ts",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api"
        }
    ],
    "crons": [
        {
            "path": "/api/contact/cron",
            "schedule": "*/5 * * * *"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}